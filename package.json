{
  "name": "@vtrbo/cli",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.33.6",
  "description": "Victor Bo's tool to start a project.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/cli.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/cli/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "bin": {
    "vtr": "bin/vtr.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "rimraf dist && temir src/index.ts",
    "build": "rimraf dist && temir build src/index.ts",
    "start": "pnpm run build && node dist/main.js",
    "release": "bumpp && npm publish --access=public",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@temir/cli": "^0.0.9",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.17.5",
    "@vtrbo/eslint-config": "^0.3.10",
    "@vtrbo/utils": "^0.0.15",
    "bumpp": "^8.2.1",
    "chalk": "^5.3.0",
    "commander": "^9.5.0",
    "eslint": "^8.47.0",
    "inquirer": "^9.2.10",
    "load-repo": "^0.0.1",
    "ora": "^6.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "vitest": "^0.22.1"
  }
}
