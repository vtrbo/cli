{
  "name": "@vtrbo/cli",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.8.0",
  "description": "Victor Bo's tool to start a project.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/cli.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/cli/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "vtr": "bin/index.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "esno src/index.ts",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp && npm publish --access=public",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@types/prompts": "^2.0.14",
    "@vtrbo/eslint-config": "^0.0.6",
    "@vtrbo/ni": "^0.0.3",
    "@vtrbo/utils": "0.0.11",
    "bumpp": "^8.2.1",
    "chalk": "^5.0.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.11",
    "load-repo": "^0.0.1",
    "ora": "^6.1.2",
    "pnpm": "^7.8.0",
    "prompts": "^2.4.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vitest": "^0.21.0"
  }
}
