{
  "name": "@vtrbo/cli",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@7.9.0",
  "description": "Victor Bo's tool to start a project.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/cli.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/cli/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "vtr": "bin/index.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "temir src/main.ts",
    "build": "temir build src/main.ts",
    "start": "pnpm run build && node dist/main.js",
    "release": "bumpp && npm publish --access=public",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@temir/cli": "^0.0.6",
    "@temir/core": "^0.0.6",
    "@types/node": "^18.7.1",
    "@vtrbo/eslint-config": "^0.0.6",
    "@vtrbo/ni": "^0.0.3",
    "@vtrbo/utils": "0.0.13",
    "@vue/runtime-core": "^3.2.37",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "load-repo": "^0.0.1",
    "pnpm": "^7.9.0",
    "typescript": "^4.7.4",
    "vitest": "^0.21.1",
    "vue": "^3.2.37"
  }
}
