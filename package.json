{
  "name": "@vtrbo/cli",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@7.9.0",
  "description": "Victor Bo's tool to start a project.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/cli.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/cli/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/main.cjs"
    }
  },
  "main": "dist/main.cjs",
  "bin": {
    "vtr": "bin/vtr.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "temir src/main.ts",
    "build": "temir build src/main.ts",
    "start": "pnpm run build && node dist/main.js",
    "release": "bumpp && npm publish --access=public",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@temir/cli": "^0.0.9",
    "@temir/core": "^0.0.9",
    "@temir/link": "^0.0.9",
    "@temir/select-input": "^0.0.9",
    "@temir/spinner": "^0.0.9",
    "@temir/tab": "^0.0.9",
    "@vitejs/plugin-vue": "^3.0.2",
    "@vtrbo/eslint-config": "^0.0.6",
    "@vue/runtime-core": "^3.2.37",
    "eslint": "^8.21.0",
    "vite": "^3.0.6",
    "vite-node": "^0.21.1",
    "vue": "^3.2.37"
  }
}
